<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <x:Double x:Key="SizeXS">4</x:Double>
    <x:Double x:Key="SizeS">8</x:Double>
    <x:Double x:Key="SizeM">16</x:Double>
    <x:Double x:Key="SizeL">32</x:Double>
    <x:Double x:Key="SizeXL">64</x:Double>

    <Size x:Key="SizeSquishS" Height="{StaticResource SizeXS}" Width="{StaticResource SizeS}"/>
    <Size x:Key="SizeSquishM"  Height="{StaticResource SizeS}" Width="{StaticResource SizeM}"/>
    <Size x:Key="SizeSquishL" Height="{StaticResource SizeM}" Width="{StaticResource SizeL}"/>
    <Size x:Key="SizeSquishXL" Height="{StaticResource SizeL}" Width="{StaticResource SizeXL}"/>
</ResourceDictionary>