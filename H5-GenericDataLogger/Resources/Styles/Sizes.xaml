<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
	<x:Double x:Key="SizeXXS">4.74</x:Double>
	<x:Double x:Key="SizeXS">7.11</x:Double>
	<x:Double x:Key="SizeS">10.67</x:Double>
    <x:Double x:Key="SizeM">16.00</x:Double>
    <x:Double x:Key="SizeL">24.00</x:Double>
    <x:Double x:Key="SizeXL">36.00</x:Double>
    <x:Double x:Key="SizeXXL">54.00</x:Double>

    <Size x:Key="SizeSquishS" Height="{StaticResource SizeXS}" Width="{StaticResource SizeS}"/>
    <Size x:Key="SizeSquishM"  Height="{StaticResource SizeS}" Width="{StaticResource SizeM}"/>
    <Size x:Key="SizeSquishL" Height="{StaticResource SizeM}" Width="{StaticResource SizeL}"/>
    <Size x:Key="SizeSquishXL" Height="{StaticResource SizeL}" Width="{StaticResource SizeXL}"/>
</ResourceDictionary>