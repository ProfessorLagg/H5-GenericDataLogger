<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

	<!-- Note: For Android please see also Platforms\Android\Resources\values\colors.xml -->

	<Color x:Key="Blue10">#ccdeff</Color>
	<Color x:Key="Blue30">#99bdff</Color>
	<Color x:Key="Blue50">#0059ff</Color>
	<Color x:Key="Blue70">#003699</Color>
	<Color x:Key="Blue90">#001233</Color>

	<Color x:Key="Orange10">#ffeecc</Color>
	<Color x:Key="Orange30">#ffcc66</Color>
	<Color x:Key="Orange50">#ffaa00</Color>
	<Color x:Key="Orange70">#996600</Color>
	<Color x:Key="Orange90">#332200</Color>
	
	<Color x:Key="Red10">#fcd6cf</Color>
	<Color x:Key="Red30">#f6836f</Color>
	<Color x:Key="Red50">#f2330d</Color>
	<Color x:Key="Red70">#901d09</Color>
	<Color x:Key="Red90">#300a03</Color>

	<Color x:Key="White">#FFFFFF</Color>
	<Color x:Key="Black">#000000</Color>

	<Color x:Key="Gray05">#f2f2f2</Color>
	<Color x:Key="Gray10">#e6e6e6</Color>
	<Color x:Key="Gray20">#cccccc</Color>
	<Color x:Key="Gray70">#4d4d4d</Color>
	<Color x:Key="Gray80">#333333</Color>
	<Color x:Key="Gray90">#1a1a1a</Color>

	<Color x:Key="Opaque">#00000000</Color>


	<SolidColorBrush x:Key="Blue10Brush" Color="{StaticResource Blue10}"/>
	<SolidColorBrush x:Key="Blue30Brush" Color="{StaticResource Blue30}"/>
	<SolidColorBrush x:Key="Blue50Brush" Color="{StaticResource Blue50}"/>
	<SolidColorBrush x:Key="Blue70Brush" Color="{StaticResource Blue70}"/>
	<SolidColorBrush x:Key="Blue90Brush" Color="{StaticResource Blue90}"/>
	
	<SolidColorBrush x:Key="Orange10Brush" Color="{StaticResource Orange10}"/>
	<SolidColorBrush x:Key="Orange30Brush" Color="{StaticResource Orange30}"/>
	<SolidColorBrush x:Key="Orange50Brush" Color="{StaticResource Orange50}"/>
	<SolidColorBrush x:Key="Orange70Brush" Color="{StaticResource Orange70}"/>
	<SolidColorBrush x:Key="Orange90Brush" Color="{StaticResource Orange90}"/>

	<SolidColorBrush x:Key="Red10Brush" Color="{StaticResource Red10}"/>
	<SolidColorBrush x:Key="Red30Brush" Color="{StaticResource Red30}"/>
	<SolidColorBrush x:Key="Red50Brush" Color="{StaticResource Red50}"/>
	<SolidColorBrush x:Key="Red70Brush" Color="{StaticResource Red70}"/>
	<SolidColorBrush x:Key="Red90Brush" Color="{StaticResource Red90}"/>
	

	<SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>
	<SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}"/>

	<SolidColorBrush x:Key="Gray05Brush" Color="{StaticResource Gray05}"/>
	<SolidColorBrush x:Key="Gray10Brush" Color="{StaticResource Gray10}"/>
	<SolidColorBrush x:Key="Gray20Brush" Color="{StaticResource Gray20}"/>
	<SolidColorBrush x:Key="Gray70Brush" Color="{StaticResource Gray70}"/>
	<SolidColorBrush x:Key="Gray80Brush" Color="{StaticResource Gray80}"/>
	<SolidColorBrush x:Key="Gray90Brush" Color="{StaticResource Gray90}"/>

	<SolidColorBrush x:Key="OpaqueBrush" Color="{StaticResource Opaque}"/>

</ResourceDictionary>